.stepper {
	width: 208rpx;
	height: 60rpx;
	/*给主容器设一个边框*/
	border: 1px solid #E6E6E6;
	border-radius: 10rpx;
	margin:0 auto;
  margin-right: 0;
  float: right;
}
/*加号和减号*/
.stepper text {
	width: 58rpx;
	line-height: 60rpx;
	text-align: center;
	float: left;
}
 
/*数值*/
.stepper input {
	width: 88rpx;
	height: 60rpx;
	float: left;
	margin: 0 auto;
	text-align: center;
	font-size: 30rpx;
	/*给中间的input设置左右边框即可*/
	border-left: 1px solid #E6E6E6;
	border-right: 1px solid #E6E6E6;
}
 
/*普通样式*/
.stepper .normal{
	color: black;
}
