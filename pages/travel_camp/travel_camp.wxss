  color: #fff;
}
.camp_dd1{
  color: #101010;
  font-size: 32rpx;
  font-weight: bold;
  line-height: 32rpx;
  margin-top: 22rpx;
}
.camp_dd1 image{
  width: 30rpx;
  height: 27rpx;
  float: right;
}
.camp_dd2{
  color: #8A8F9B;
  font-size: 24rpx;
