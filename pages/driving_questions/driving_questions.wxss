.questions{
  padding: 50rpx 40rpx;
  background: #fff;
}
.quest_con{
  font-size: 34rpx;
  line-height: 50rpx;
}
.quest_options{
  font-size: 34rpx;
  color: #8A8F9B;
  line-height: 90rpx;
}
.qtext{
  display: inline-block;
  font-size: 28rpx;
  color: #000000;
  width: 50rpx;
  height: 50rpx;
  line-height: 50rpx;
  border: 1px solid #DDDDDD;
  text-align: center;
  border-radius: 50%;
  margin-right: 20rpx;
}

.is_checked {
   color:#FFA826;
}
.disno{
  display: none;
}
.diblock{
  display: inline-block;
}
radio{
  margin-right: 20rpx;
}
radio-group label{
  display: block;
}
.quest_boot{
  width:100%;
  height: 94rpx;
  line-height: 94rpx;
  background: #fff;
  text-align: center;
  overflow: hidden;
  position: fixed;
  bottom: 0;
  left: 0;
}
.que_now{
  color: #010101;
  font-size: 30rpx;
  margin-left: 90rpx;
}
.que_all{
  color: #9B9FA9;
  font-size: 30rpx;
}
.quest_dl{
  float: right;
  width:60rpx;
  height: 94rpx;
  margin-right: 30rpx;
}
.quest_dt{
  width:100%;
  height: 45rpx;
  line-height: 45rpx;
  text-align: center;
}
.quest_dt image{
  width:28rpx;
  height: 28rpx;
  margin-top: 20rpx;
}
.quest_dd{
  color: #8A8F9B;
  line-height: 45rpx;
  height: 45rpx;
  font-size: 20rpx;
  text-align: center;
}
.jujue_tc{position: fixed;top: 0;width: 100%;height: 100%;background: rgba(0, 0, 0, 0.3);z-index: 99}
.jujue_cont{width: 560rpx;background: #fff;margin: 0 auto; padding-bottom:136rpx; border-radius: 20rpx;margin-top: 45%;text-align: center;position: relative;}
.jujue_title{font-size: 36rpx;display:block;height: 60rpx;line-height: 60rpx;padding-top: 80rpx; color: #333333;margin-bottom: 20rpx;}
.jujues{ font-size: 30rpx; color: #333333; line-height: 50rpx;}
.btn_jujue{position: absolute;bottom: 0;display: flex;flex-direction: row;width: 100%;height:86rpx;line-height: 86rpx;border-top: 1px solid #E6E6E6; }
.btn_jujue text{width: 50%;line-height: 86rpx;font-size: 30rpx;}
.success{color: #fff; background: #FFA826; border-bottom-right-radius: 20rpx;}
.disno{
  display: none;
}
radio-group .quest_options:first-child{
  margin-top: 40rpx;
}